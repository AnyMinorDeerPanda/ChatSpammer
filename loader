load = function()
  -- Load Window Renderer
  var XSpam = loadstring(game:HttpGet('https://raw.githubusercontent.com/AnyMinorDeerPanda/ChatSpammer/main/WindowRenderer.lua'))()

  -- Load Main Script
  loadstring(game:HttpGet('https://raw.githubusercontent.com/AnyMinorDeerPanda/ChatSpammer/main/MainScript.lua'))(XSpam)
end

return load
